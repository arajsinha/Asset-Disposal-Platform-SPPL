{
  "xsappname": "asset-disposal-singaporepools",
  "tenant-mode": "dedicated",
  "oauth2-configuration": {
    "redirect-uris": [
      "http://localhost:5001/",
      "http://localhost:5002/",
      "http://localhost:5003/",
      "http://localhost:5004/",
      "http://localhost:5005/",
      "http://localhost:8080/",
      "https://*.sap/login/callback"
    ]
  },
  "description": "Security profile of called application",
  "scopes": [
    {
      "name": "$XSAPPNAME.user",
      "description": "UAA",
      "granted-apps" : [ "$XSAPPNAME(application,assetdisposaltask)"]
    }
  ],
  "role-templates": [
    {
      "name": "Token_Exchange",
      "description": "UAA",
      "scope-references": [
        "$XSAPPNAME.user"
      ]
    }
  ],
  "attributes": []
}
