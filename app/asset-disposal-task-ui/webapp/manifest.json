{
    "_version": "1.26.0",
    "sap.app": {
        "id": "assetdisposaltaskui",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "0.0.3"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "resources": "resources.json",
        "sourceTemplate": {
            "id": "@sap/generator-fiori:basic",
            "version": "1.8.4",
            "toolsId": "b42304a0-fcda-45fa-af38-14b74d2f3ada"
        },
        "dataSources": {
            "mainService": {
                "uri": "/odata/v4/asset-disposal/",
                "type": "OData",
                "settings": {
                    "annotations": [],
                    "localUri": "localService/metadata.xml",
                    "odataVersion": "4.0"
                }
            }
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "flexEnabled": false,
        "dependencies": {
            "minUI5Version": "1.82.2",
            "libs": {
                "sap.m": {},
                "sap.ui.core": {},
                "sap.f": {},
                "sap.suite.ui.generic.template": {},
                "sap.ui.comp": {},
                "sap.ui.generic.app": {},
                "sap.ui.table": {},
                "sap.ushell": {}
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "assetdisposaltaskui.i18n.i18n"
                }
            },
            "": {
                "dataSource": "mainService",
                "preload": true,
                "settings": {
                    "synchronizationMode": "None",
                    "operationMode": "Server",
                    "autoExpandSelect": true,
                    "earlyRequests": true
                }
            }
        },
        "resources": {
            "css": [
                {
                    "uri": "css/style.css"
                }
            ]
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "viewType": "XML",
                "async": true,
                "viewPath": "assetdisposaltaskui.view",
                "controlAggregation": "pages",
                "controlId": "app",
                "clearControlAggregation": false
            },
            "routes": [
                {
                    "name": "RouteGenericTaskUI",
                    "pattern": ":?query:",
                    "target": [
                        "TargetGenericTaskUI"
                    ]
                }
            ],
            "targets": {
                "TargetGenericTaskUI": {
                    "viewType": "XML",
                    "transition": "slide",
                    "clearControlAggregation": false,
                    "viewId": "GenericTaskUI",
                    "viewName": "GenericTaskUI"
                }
            }
        },
        "rootView": {
            "viewName": "assetdisposaltaskui.view.App",
            "type": "XML",
            "async": true,
            "id": "App"
        }
    },
    "sap.cloud": {
        "public": true,
        "service": "spools.asset.disposal.request"
    },
    "sap.bpa.task": {
        "_version": "1.1.0",
        "outcomes": [
            {
                "id": "approve",
                "label": "Approve",
                "type": "positive"
            },
            {
                "id": "reject",
                "label": "Reject",
                "type": "negative"
            },
            {
                "id": "void",
                "label": "Void",
                "type": "negative"
            }
        ],
        "inputs": {
            "$schema": "http://json-schema.org/draft-07/schema",
            "title": "input",
            "type": "object",
            "required": [
                "requestId",
                "taskType"
            ],
            "properties": {
                "requestId": {
                    "type": "string",
                    "title": "Request ID",
                    "description": "Request ID"
                },
                "taskType": {
                    "title": "Task type",
                    "description": "Task type",
                    "type": "string"
                },
                "attachmentEditable": {
                    "title": "Is attachment editable?",
                    "description": "",
                    "type": "string"
                },
                "canVoid": {
                    "title": "Can void?",
                    "description": "",
                    "type": "string"
                }
            }
        },
        "outputs": {
            "$schema": "http://json-schema.org/draft-07/schema",
            "title": "output",
            "type": "object",
            "properties": {
                "comment": {
                    "type": "string",
                    "title": "Comment",
                    "description": "Comment"
                },
                "approver": {
                    "type": "string",
                    "title": "Approver",
                    "description": "Approver"
                }
            }
        },
        "category": "approval"
    }
}