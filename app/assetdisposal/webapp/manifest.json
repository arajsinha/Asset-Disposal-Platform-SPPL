{
  "_version": "1.65.0",
  "sap.app": {
    "id": "ns.assetdisposal",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:lrop",
      "version": "1.15.0",
      "toolsId": "3773919a-13d3-41dc-8006-52466b55013c"
    },
    "dataSources": {
      "mainService": {
        "uri": "/odata/v4/asset-disposal/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "odataVersion": "4.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.128.1",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.fe.templates": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "ns.assetdisposal.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      },
      "@i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "uri": "i18n/i18n.properties"
      }
    },
    "resources": {
      "css": []
    },
    "routing": {
      "config": {},
      "routes": [
        {
          "pattern": ":?query:",
          "name": "RequestDetailsList",
          "target": "RequestDetailsList"
        },
        {
          "pattern": "RequestDetails({key}):?query:",
          "name": "RequestDetailsObjectPage",
          "target": "RequestDetailsObjectPage"
        }
      ],
      "targets": {
        "RequestDetailsList": {
          "type": "Component",
          "id": "RequestDetailsList",
          "name": "sap.fe.templates.ListReport",
          "options": {
            "settings": {
              "contextPath": "/RequestDetails",
              "variantManagement": "Page",
              "navigation": {
                "RequestDetails": {
                  "detail": {
                    "route": "RequestDetailsObjectPage"
                  }
                }
              },
              "controlConfiguration": {
                "@com.sap.vocabularies.UI.v1.LineItem": {
                  "tableSettings": {
                    "type": "ResponsiveTable"
                  }
                }
              }
            }
          }
        },
        "RequestDetailsObjectPage": {
          "type": "Component",
          "id": "RequestDetailsObjectPage",
          "name": "sap.fe.templates.ObjectPage",
          "options": {
            "settings": {
              "editableHeaderContent": false,
              "contextPath": "/RequestDetails",
              "controlConfiguration": {
                "assetDetails/@com.sap.vocabularies.UI.v1.LineItem#Assets": {
                  "columns": {
                    "DataField::requestDetails::assetDetails::assetDesc": {
                      "width": "20%"
                    },
                    "DataField::subNumber": {
                      "width": "5%"
                    },
                    "DataField::assetClass": {
                      "width": "8%"
                    },
                    "DataField::costCenter": {
                      "width": "8%"
                    },
                    "DataField::assetPurchaseDate": {
                      "width": "8%"
                    },
                    "DataField::netBookValue": {
                      "width": "8%"
                    },
                    "DataField::disposalMethod": {
                      "width": "8%"
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  },
  "sap.fiori": {
    "registrationIds": [],
    "archeType": "transactional"
  }
}
